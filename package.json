{
    "name": "cap-launchpad",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^4.4.10",
        "@sap/hana-client": "^2.7.21",
        "@sap/xsenv": "^3.1.0",
        "@sap/xssec": "^3.0.10",
        "express": "^4.17.1",
        "passport": "^0.4.1"
    },
    "devDependencies": {
        "@sap/ux-specification": "^1.84.1",
        "sqlite3": "5.0.0"
    },
    "scripts": {
        "start": "npx cds run",
        "build": "mbt build -p=cf",
        "deploy": "cf deploy mta_archives/cap-launchpad_1.0.0.mtar",
        "deploy:hana": "cf deploy mta_archives/cap-launchpad_1.0.0.mtar -e cf.mtaext",
        "undeploy": "cf undeploy cap-launchpad --delete-services --delete-service-brokers --delete-service-keys"
    },
    "sapux": [
        "app/fiori"
    ],
    "cds": {
        "hana": {
            "deploy-format": "hdbtable"
        },
        "requires": {
            "db": {
                "kind": "sql"
            },
            "uaa": {
                "kind": "xsuaa"
            }
        }
    }
}
