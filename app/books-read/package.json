{
    "name": "books-read",
    "version": "0.0.1",
    "description": "Read Books",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "scripts": {
        "deploy": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
        "build": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateManifestBundle generateCachebusterInfo"
    },
    "devDependencies": {
        "@ui5/cli": "^2.10.3",
        "@ui5/fs": "^2.0.6",
        "@ui5/logger": "^2.0.1",
        "@sap/ux-ui5-tooling": "^1.1.10",
        "rimraf": "^3.0.2",
        "@sap/ux-specification": "latest",
        "@sap/ui5-builder-webide-extension": "^1.1.4",
        "ui5-task-zipper": "^0.4.3",
        "mbt": "^1.1.1"
    },
    "ui5": {
        "dependencies": [
            "@sap/ui5-builder-webide-extension",
            "ui5-task-zipper",
            "mbt"
        ]
    }
}
